{
    "cli": {
        "version": ">= 5.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "ios": {
                "simulator": true
            },
            "android": {
                "buildType": "apk"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview",
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "channel": "production",
            "autoIncrement": true,
            "ios": {
                "buildNumber": "1"
            },
            "android": {
                "buildType": "app-bundle"
            }
        }
    },
    "submit": {
        "production": {
            "ios": {
                "appleId": "YOUR_APPLE_ID",
                "ascAppId": "YOUR_ASC_APP_ID",
                "appleTeamId": "YOUR_TEAM_ID"
            },
            "android": {
                "serviceAccountKeyPath": "./google-service-account.json",
                "track": "internal"
            }
        }
    }
}